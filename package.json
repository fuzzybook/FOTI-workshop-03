{
  "name": "rollup-experiment",
  "version": "1.0.0",
  "description": "RollUp experiment",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuzzybook/rollup-experiment.git"
  },
  "scripts": {
    "start": "gulp",
    "lint": "eslint ./src/**/*.js --ext js",
    "clean": "rm -rf ./dist && mkdir dist",
    "lint:fix": "eslint --fix ./src/**/*.js",
    "test": "jest --watchAll",
    "test:build": "jest",
    "jest:coverage": "jest --coverage"
  },
  "keywords": [
    "RollUp"
  ],
  "author": "Fuzzybook",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fuzzybook/rollup-experiment/issues"
  },
  "homepage": "https://github.com/fuzzybook/rollup-experiment#readme",
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babelrc-rollup": "^3.0.0",
    "browser-sync": "^2.26.3",
    "eslint": "4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "event-emitter": "^0.3.5",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-babel": "6.1.2",
    "gulp-htmlmin": "3.0.0",
    "gulp-imagemin": "3.3.0",
    "gulp-minify-css": "1.2.4",
    "gulp-plumber": "1.1.0",
    "gulp-sass": "3.1.0",
    "gulp-uglify": "3.0.0",
    "jest": "^23.4.2",
    "jest-mock-console": "^0.4.0",
    "pump": "1.0.2",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-browsersync": "^0.2.6",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify-es": "0.0.1"
  },
  "dependencies": {
    "express": "^4.16.4",
    "hyperhtml": "^2.13.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "modulePaths": [
      "src"
    ]
  }
}
